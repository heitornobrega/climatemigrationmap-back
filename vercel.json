{
    "version": 2,
    "builds": [
      {
        "src": "api/app.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "api/app.py"
      }
    ],
    "env": {
      "FLASK_ENV": "production",
      "FLASK_APP": "app.py"
    },
    "server": {
      "command": "flask run --host 0.0.0.0 --port $PORT"
    }
  }
  